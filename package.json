    "axios": "^1.7.7",
    "expo": "~51.0.39",
    "expo-font": "~12.0.10",
    "expo-status-bar": "~1.12.1",
    "jest-expo": "~51.0.4",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.16.1",
